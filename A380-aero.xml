                              0.0400	0.8500
                              0.2300	1.3000
                              0.6000	0.7000
                          </tableData>
                          <!-- tableData>
                              -0.2000	-0.6800
                              0.0000	0.3000
                              0.2300	1.2000
                              0.6000	0.7000
                          </tableData -->
                          <!-- 0.0000 0.2000
                               0.2300 1.2000
                               0.6000 0.6000  -->
                      </table>
                </product>
            </function>
            
            <!-- orig property>fcs/flap-pos-norm</property -->

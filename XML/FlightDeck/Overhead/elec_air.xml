	<!-- APU gen -->
    <animation>
          <type>textranslate</type>
          <object-name>APU_GENA</object-name>
          <property>/controls/electric/contact/apu_gen-a</property>
          <axis>
              <x>0</x>
              <y>0.6</y>
              <z>1</z>
          </axis>
    </animation>
    <animation>
          <type>material</type>
          <object-name>APU_GENA</object-name>
          <condition>
            <equals>
              <property>/controls/electric/contact/apu_gen-a</property>
              <value>0</value>
            </equals>
          </condition>
          <emission>
                <red>0.3</red>
                <green>0.3</green>
                <blue>0.3</blue>
          </emission>
    </animation>
    <animation>
      <type>pick</type>
      <object-name>APU_GENA</object-name>
      <action>
        <button>0</button>
	<binding>
          <command>property-toggle</command>
          <property>/controls/electric/contact/apu_gen-a</property>
        </binding>
      </action>
    </animation>
    

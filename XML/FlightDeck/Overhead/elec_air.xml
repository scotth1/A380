	<!-- APU bus tie -->
    <animation>
          <type>textranslate</type>
          <object-name>APU_GENA</object-name>
          <object-name>APU_GENB</object-name>
          <property>/controls/electric/engine[4]/bus-tie</property>
          <axis>
              <x>0</x>
              <y>0.6</y>
              <z>1</z>
          </axis>
    </animation>
    <animation>
          <type>material</type>
          <object-name>APU_GENA</object-name>
          <object-name>APU_GENB</object-name>
          <property>/controls/electric/engine[4]/bus-tie</property>
          <emission>
                <red>0.0</red>
                <green>0.2</green>
                <blue>0.0</blue>
                <factor-prop>/controls/electric/engine[4]/bus-tie</factor-prop>
          </emission>
    </animation>
    

    
